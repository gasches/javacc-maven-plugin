import java.io.*;
import java.util.*;
import org.codehaus.plexus.util.*;

try
{
    File file = new File( basedir, "target/site/jjdoc/BasicParser.html" );
    if ( !file.exists() || file.isDirectory() )
    {
        System.err.println( "Could not find generated jjdoc file: " + file );
        return false;
    }
}
catch( Throwable t )
{
    t.printStackTrace();
    return false;
}

return true;
